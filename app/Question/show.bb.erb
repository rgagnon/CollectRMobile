  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">SectionId</td>
        <td class="itemValue"><%= @question.section_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">DataTypeId</td>
        <td class="itemValue"><%= @question.data_type_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">LabelFr</td>
        <td class="itemValue"><%= @question.label_fr%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">LabelEn</td>
        <td class="itemValue"><%= @question.label_en%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">HelpFr</td>
        <td class="itemValue"><%= @question.help_fr%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">HelpEn</td>
        <td class="itemValue"><%= @question.help_en%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">IsMultipleChoice</td>
        <td class="itemValue"><%= @question.is_multiple_choice%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">EffectiveDate</td>
        <td class="itemValue"><%= @question.effective_date%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">EndDate</td>
        <td class="itemValue"><%= @question.end_date%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Order</td>
        <td class="itemValue"><%= @question.order%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Precision</td>
        <td class="itemValue"><%= @question.precision%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Scale</td>
        <td class="itemValue"><%= @question.scale%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">MinValue</td>
        <td class="itemValue"><%= @question.min_value%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">MaxValue</td>
        <td class="itemValue"><%= @question.max_value%></td>
      </tr>
      

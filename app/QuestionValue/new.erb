<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @questionvalue.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @questionvalue.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="questionvalue[question_id]" class="fieldLabel">Question</label>
            <input type="text" id="questionvalue[question_id]" name="questionvalue[question_id]" <%= placeholder("Question") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="questionvalue[value_fr]" class="fieldLabel">Value fr</label>
            <input type="text" id="questionvalue[value_fr]" name="questionvalue[value_fr]" <%= placeholder("Value fr") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="questionvalue[value_en]" class="fieldLabel">Value en</label>
            <input type="text" id="questionvalue[value_en]" name="questionvalue[value_en]" <%= placeholder("Value en") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="questionvalue[order]" class="fieldLabel">Order</label>
            <input type="text" id="questionvalue[order]" name="questionvalue[order]" <%= placeholder("Order") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="questionvalue[effective_date]" class="fieldLabel">Effective date</label>
            <input type="text" id="questionvalue[effective_date]" name="questionvalue[effective_date]" <%= placeholder("Effective date") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="questionvalue[end_date]" class="fieldLabel">End date</label>
            <input type="text" id="questionvalue[end_date]" name="questionvalue[end_date]" <%= placeholder("End date") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>

<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @questionvalue.question_id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @questionvalue.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">QuestionId</td>
        <td class="itemValue"><%= @questionvalue.question_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ValueFr</td>
        <td class="itemValue"><%= @questionvalue.value_fr%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ValueEn</td>
        <td class="itemValue"><%= @questionvalue.value_en%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Order</td>
        <td class="itemValue"><%= @questionvalue.order%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">EffectiveDate</td>
        <td class="itemValue"><%= @questionvalue.effective_date%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">EndDate</td>
        <td class="itemValue"><%= @questionvalue.end_date%></td>
      </tr>
      
    </table>
  </div>

</div>

<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New QuestionValue</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @questionvalue.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Question: </td>
                <td class="itemValue"><input type="text" name="questionvalue[question_id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Value fr: </td>
                <td class="itemValue"><input type="text" name="questionvalue[value_fr]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Value en: </td>
                <td class="itemValue"><input type="text" name="questionvalue[value_en]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Order: </td>
                <td class="itemValue"><input type="text" name="questionvalue[order]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Effective date: </td>
                <td class="itemValue"><input type="text" name="questionvalue[effective_date]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">End date: </td>
                <td class="itemValue"><input type="text" name="questionvalue[end_date]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
